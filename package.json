    "fcbuffer": "^2.2.1",
    "is-node": "1.0.2",
    "isomorphic-fetch": "^2.2.1",
    "lodash.isempty": "^4.4.0",
    "lodash.isstring": "^4.0.1",
    "minimatch": "^3.0.4",
    "randombytes": "^2.0.5"
  }
}
